<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="stu" class="cp01.Student">
        <property name="id" value="1"></property>
        <property name="name" value="Jason"></property>
        <property name="age" value="28"></property>
    </bean>

    <bean id="stux" class="cp01.Student">
        <constructor-arg index="0" value="2"></constructor-arg>
        <constructor-arg index="1" value="Lolita"></constructor-arg>
        <constructor-arg index="3" value="24"></constructor-arg>
    </bean>
</beans>